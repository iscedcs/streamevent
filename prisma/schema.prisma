generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Wish {
  id        String   @id @default(cuid())
  userId    String
  name      String
  image     String
  message   String
  createdAt DateTime @default(now())
}

